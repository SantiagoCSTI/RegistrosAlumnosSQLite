<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="280dp"
        android:background="@drawable/fotoalumno" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="tvnombres_ROW"
        android:id="@+id/tvnombres_ROW"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tvFechaNacimiento_ROW"
        android:id="@+id/tvFechaNacimiento_ROW"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignEnd="@+id/tvnombres_ROW" />
</RelativeLayout>
